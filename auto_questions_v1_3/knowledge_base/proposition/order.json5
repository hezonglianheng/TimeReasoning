// 与时间命题相关的知识库文件，表示事件的次序

{
    "prop_kinds": {
        // 时点事件命题
        "temporal": {
            "templates": {
                "cn": [
                    "{event:}是{time:}个发生的事件",
                    "{event:}是发生在{time:}的事件",
                    "{event:}{time:}个发生",
                ],
                "en": [
                    "{event:} is {time:} event that happened",
                ],
            },
        },
        "before": {
            "templates": {
                "cn": [
                    "{event1:}早于{event2:}",
                    "{event1:}的时间比{event2:}早",
                ],
                "en": [
                    "{event1:} is earlier than {event2:}",
                ],
            },
        }, 
        "after": {
            "templates": {
                "cn": [
                    "{event1:}晚于{event2:}",
                    "{event1:}的时间比{event2:}晚",
                ],
                "en": [
                    "{event1:} is later than {event2:}",
                ],
            },
        },
    },
}